{
    "name": "uu4hyu/slim4-tutorial",
    "description": "Tutorial using Slim php framework 4",
    "type": "project",
    "require": {
        "slim/slim": "^4.4",
        "slim/psr7": "^1.0",
        "slim/http": "^1.0",
        "php-di/php-di": "^6.0",
        "selective/config": "^0.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
